	var lazyload = function(){
		var scrollTop = document.body.scrollTop || document.documentElement.scrollTop;
		var winTop = window.innerHeight;
		for(var i=0;i < imgs.length;i++){
			if(imgs[i].offsetTop < scrollTop + winTop && imgs[i].src == "" ){
				imgs[i].src = imgs[i].getAttribute('data-src');
				imgs[i].classList.remove("lazy-load");
			}
		}
	}
	function throttle(method,delay){
		var timer = null;
		return function(){
			var context = this, args=arguments;
			clearTimeout(timer);
			timer=setTimeout(function(){method.apply(context,args);},delay);
		}
	}
	window.onload = throttle(lazyload,200);
    window.onscroll = throttle(lazyload,200);
